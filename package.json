{
  "name": "raydium-swap-example",
  "version": "1.0.0",
  "description": "A volume bot that swaps tokens on Solana using the Raydium SDK.",
  "type": "commonjs",
  "main": "index.ts",
  "scripts": {
    "setup": "npx ts-node production-setup.ts",
    "setup:production": "npx ts-node production-ready-setup.ts",
    "swap": "node --max-old-space-size=4096 -r ts-node/register index.ts",
    "test": "npx ts-node test-functionality.ts",
    "test:all": "npx ts-node test-dependencies.ts && npx ts-node test-functionality.ts && npx ts-node test-main-flow.ts",
    "api": "API_PORT=12001 npx ts-node simple-api-server.ts",
    "api:prod": "NODE_ENV=production API_PORT=12001 npx ts-node simple-api-server.ts",
    "dev": "concurrently \"npm run api\" \"cd web-dashboard && npm run dev\"",
    "dev:prod": "concurrently \"npm run api:prod\" \"cd web-dashboard && npm run dev\"",
    "build": "cd web-dashboard && npm run build",
    "build:all": "npm install && cd web-dashboard && npm install && npm run build",
    "start": "npm run api:prod",
    "start:dev": "npm run dev",
    "start:production": "pm2 start ecosystem.config.js",
    "stop:production": "pm2 stop ecosystem.config.js",
    "restart:production": "pm2 restart ecosystem.config.js",
    "logs": "pm2 logs",
    "monitor": "pm2 monit"
  },
  "author": "Gamblers_Passion",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@openbook-dex/openbook": "^0.0.9",
    "@project-serum/serum": "^0.13.65",
    "@raydium-io/raydium-sdk": "^1.3.1-beta.52",
    "@solana-developers/helpers": "^2.3.0",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.92.3",
    "async-mutex": "^0.5.0",
    "axios": "^1.7.2",
    "bs58": "^5.0.0",
    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "keypress": "^0.2.1",
    "node-fetch": "^2.7.0",
    "prompt-sync": "^4.2.0",
    "readline": "^1.3.0",
    "socket.io": "^4.7.4",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/node": "^20.11.30",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2"
  }
}
